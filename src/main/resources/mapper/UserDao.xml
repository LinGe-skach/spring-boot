<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.gridvo.cloud.dao.UserDao">
    <resultMap id="mapper" type="com.example.gridvo.cloud.entity.vo.UserInfoVO">
        <id column="id" property="id"/>
        <result property="username" column="username"/>
        <result column="password" property="password"/>
    </resultMap>
    <insert id="insert" parameterType="com.example.gridvo.cloud.entity.vo.UserInfoVO">
        insert into user_info id=#{id},username=#{username},password=#{password}
    </insert>
    <select id="selectByUserName" resultMap="mapper">
        select user_info id ,username,password, from user_info where username=#{username}
    </select>

</mapper>